{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["1self.js", "main.js", "eventPage.js"],
        "persistent": true
    }
}
